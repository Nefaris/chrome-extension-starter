{
  "manifest_version": 2,
  "name": "Vol kozak",
  "version": "0.1",
  "description": "Vol kozak desc",
  "permissions": [
    "tabs",
    "activeTab",
    "tabCapture"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.bundle.js"
    ]
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://*/*"
      ],
      "js": [
        "content.bundle.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}
