{
  "manifest_version": 2,
  "name": "Extension name",
  "version": "0.1",
  "description": "Extension description",
  "permissions": [],
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.bundle.js"
    ]
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [],
      "js": [
        "content.bundle.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}
